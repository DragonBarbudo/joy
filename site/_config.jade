include ../dw/dw.jade

- var domain = "index.html"

//- METAS
- var title = "JOY Comunicación"
- var keywords = ""
- var description = ""

//- STYLESHEETS
- var stylesheets = []
- stylesheets.push('http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css')
- stylesheets.push('http://fonts.googleapis.com/css?family=Lato:300,400,700')
- stylesheets.push('https://fonts.googleapis.com/css?family=Exo:300,400')
- stylesheets.push('http://cdn.jsdelivr.net/jquery.slick/1.5.7/slick.css')
- stylesheets.push('https://cdnjs.cloudflare.com/ajax/libs/jquery.swipebox/1.4.1/css/swipebox.min.css')
